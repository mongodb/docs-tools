    height: 54px;
}

ul.home-nav li.downloads { width: 245px; }

ul.home-nav li.downloads a, ul.home-nav li.downloads a:visited {
    float: left;
    background: url(http://www.mongodb.org/static/images/home_nav.png) -497px -63px;
